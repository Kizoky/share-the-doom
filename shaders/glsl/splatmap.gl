vec4 Process(vec4 fc)
{
    vec2 texCoord = vTexCoord.st;
	vec4 mask = normalize(getTexel(texCoord));
	vec4 col;
	float f;
	col.a = 1.0;
	f = mask.r;
	col += vec4((texture(tex_in1, texCoord*128).rgb)*f, 1.0);
	f = mask.g;
	col += vec4((texture(tex_in2, texCoord*128).rgb)*f, 1.0);
	f = mask.b;
	col += vec4((texture(tex_in3, texCoord*128).rgb)*f, 1.0);
	
	return col;
}